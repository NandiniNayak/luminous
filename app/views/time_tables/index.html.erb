<p id="notice"><%= notice %></p>

<h1>Time Tables</h1>

<table>
  <thead>
    <tr>
      <th>Room</th>
      <th>Subject</th>
      <th>Time</th>
      <th>Day</th>
      <th>Year</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_tables.each do |time_table| %>
      <tr>
        <td><%= time_table.room %></td>
        <td><%= time_table.subject %></td>
        <td><%= time_table.time %></td>
        <td><%= time_table.day %></td>
        <td><%= time_table.year %></td>
        <td><%= time_table.user %></td>
        <td><%= link_to 'Show', time_table %></td>
        <td><%= link_to 'Edit', edit_time_table_path(time_table) %></td>
        <td><%= link_to 'Destroy', time_table, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Table', new_time_table_path %>
